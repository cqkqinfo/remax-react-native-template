    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.2",
    "@react-navigation/stack": "5.x",
    "@types/axios-curlirize": "^1.3.1",
    "@types/qs": "^6.9.7",
    "@types/react-native": "git+https://github.com/kristerkari/react-native-types-for-css-modules.git",
    "axios": "^0.20.0",
    "axios-curlirize": "^1.3.7",
    "babel-plugin-polyfill-corejs3": "^0.2.4",
    "create-api-hooks": "^0.0.26",
    "dayjs": "^1.8.28",
    "gcoord": "^0.3.2",
    "parsec-hooks": "^0.0.157",
    "qs": "^6.10.1",
