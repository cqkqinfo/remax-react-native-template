{
  "name": "create-parsec-app-weapp",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "{{author}}",
  "description": "{{description}}",
  "repository": {
    "type": "git",
    "url": "{{git}}"
  },
  "license": "MIT",
  "scripts": {
    "dev:android": "react-native run-android",
    "dev:ios": "react-native run-ios --simulator 'iPhone 12 Pro Max'",
    "start": "react-native start",
    "dev": "yarn check-for-dupes && remax build -m -w -t",
    "dev:web": "yarn dev web",
    "dev:wechat": "yarn dev wechat",
    "build:web": "yarn build web",
    "build:wechat": "yarn build wechat",
    "build:android": "cd android && ./gradlew assembleRelease",
    "debug": " REMAX_DEBUG=true yarn dev",
    "clean": "rimraf dist && npx react-native-clean-project",
    "prebuild": "npm run clean",
    "build": "yarn check-for-dupes && cross-env NODE_ENV=production remax build -t wechat",
    "codegen": "yarn parsec-codegen",
    "icon": "npm run icon-h5&&npm run icon-wechat",
    "icon-h5": "iconfont-h5",
    "icon-wechat": "npx iconfont-remax --config iconfont.wechat.json",
    "postinstall": "patch-package",
    "check-for-dupes": "jscpd ./src -t 10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn check-for-dupes",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick --staged"
    ],
    "*.{ts,tsx,js}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@ant-design/react-native": "^4.2.0",
