{
  "name": "create-parsec-app-weapp",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "{{author}}",
  "description": "{{description}}",
  "repository": {
    "type": "git",
    "url": "{{git}}"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn check-for-dupes && remax build -w -t",
    "web": "yarn build web",
    "wechat": "yarn build wechat",
    "debug": " REMAX_DEBUG=true yarn dev",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "yarn check-for-dupes && cross-env NODE_ENV=production remax build -t wechat",
    "codegen": "yarn parsec-codegen",
    "icon": "npx iconfont-remax",
    "check-for-dupes": "jscpd ./src -t 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn check-for-dupes",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick --staged"
    ],
    "*.{ts,tsx,js}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@remax/plugin-sass": "^1.0.0",
    "@types/axios-curlirize": "^1.3.1",
    "axios": "^0.20.0",
    "axios-curlirize": "^1.3.7",
    "create-api-hooks": "^0.0.26",
    "dayjs": "^1.8.28",
    "parsec-hooks": "^0.0.130",
    "parsec-rc": "^0.0.0-alpha.49",
    "react": "^16.12.0",
    "remax": "^2.11.4"
  },
  "devDependencies": {
    "@remax/plugin-less": "^1.0.0",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.16",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-import": "^1.13.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jscpd": "^3.3.19",
    "lint-staged": "^9.5.0",
    "parsec-codegen": "^0.0.0-alpha.6",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.2",
    "validate-commit-msg": "^2.14.0"
  }
}
